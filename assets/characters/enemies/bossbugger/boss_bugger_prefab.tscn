@onready var mesh_instance_3d: MeshInstance3D = $Armature_001/Skeleton3D/BoneAttachment3D/MeshInstance3D

const FLUID_MIN: float = -0.45
const FLUID_MAX: float = 0.45

@export var fluid_level: float = 0.0:
