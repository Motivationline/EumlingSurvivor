[sub_resource type="GDScript" id="GDScript_dxrkv"]
script/source = "extends CanvasLayer

@onready var line_edit: LineEdit = $Control/CenterContainer/VBoxContainer/LineEdit

signal level_chosen(level: String)

func setup():
	show()
	line_edit.text = \"\"
	line_edit.grab_focus()

func _on_line_edit_text_submitted(_new_text: String) -> void:
	confirm_text()

func _on_button_pressed() -> void:
	confirm_text()

func confirm_text():
	hide()
	level_chosen.emit(line_edit.text)
"

[sub_resource type="GDScript" id="GDScript_l8g03"]
script/source = "extends CanvasLayer

signal area_chosen(levels: Array[String])
@onready var grid_container: GridContainer = $Control/CenterContainer/GridContainer

var areas = [
	{
		name = \"Jungle\",
		folder = \"3-Jxx-Jungle\",
		levels = [\"J01\", \"J02\", \"J03\", \"JB01\"],
	},
	{
		name = \"Volcano\",
		folder = \"6-Vxx-Volcano\",
		levels = [\"V01\"],
	},
	{
		name = \"Testing\",
		folder = \"\",
		levels = [\"001\", \"002\", \"003\", \"004\", \"005\"],
	}
]

func setup():
	for child in grid_container.get_children():
		grid_container.remove_child(child)
		child.queue_free()
	
	for area in areas:
		var btn = Button.new()
		btn.text = area.name
		grid_container.add_child(btn)
		btn.pressed.connect(choose_area.bind(area))
	
	
	show()

func choose_area(area):
	var level_names: Array[String] = []
	var folder_name: String = \"res://game/levels/\"
	if area.folder:
		folder_name +=  area.folder + \"/\"
	for level in area.levels:
		level_names.append(folder_name + level + \".tscn\")
	area_chosen.emit(level_names)
	
	hide()
"

